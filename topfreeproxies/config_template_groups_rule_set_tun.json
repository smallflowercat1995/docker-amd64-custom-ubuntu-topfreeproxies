      "type":"selector",
      "outbounds":[
        "HongKong",
        "TaiWan",
        "Singapore",
        "Japan",
        "America",
        "Others"
      ]
    },
    {
      "tag":"BiliBili",
      "type":"selector",
      "outbounds":[
        "direct",
        "HongKong",
        "TaiWan"
      ]
    },
    {
      "tag":"Bahamut",
      "type":"selector",
      "outbounds":[
        "TaiWan",
