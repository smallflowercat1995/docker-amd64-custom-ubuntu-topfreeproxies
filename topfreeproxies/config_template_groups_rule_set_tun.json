      ]
    },
    {
      "tag":"Spotify",
      "type":"selector",
      "outbounds":[
        "HongKong",
        "TaiWan",
        "Singapore",
        "Japan",
        "America",
        "Others"
      ],
      "default": "America"
    },
    {
      "tag":"TikTok",
      "type":"selector",
      "outbounds":[
        "HongKong",
        "TaiWan",
        "Singapore",
        "Japan",
        "America"
      ],
      "default": "Japan"
    },
    {
      "tag":"Netflix",
      "type":"selector",
      "outbounds":[
        "HongKong",
        "TaiWan",
        "Singapore",
        "Japan",
        "America",
        "Others"
      ]
    },
    {
      "tag":"Disney+",
      "type":"selector",
      "outbounds":[
        "HongKong",
